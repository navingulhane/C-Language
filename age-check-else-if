#include <stdio.h>

int main ()  {
    
    int age;
    
    printf("Entre your age :");
    scanf("%d", &age);
    
    if (age < 13) {
        printf("You are a kid");
        
    } else if (age >= 13 && age <=17) {
        printf("You are a teenager");
        
    } else if (age >= 18 && age <=60) {
        printf("You are an adult");
        
    } else {
        printf("You are a seniour citizen");
        
    }
    return 0;
    
}
